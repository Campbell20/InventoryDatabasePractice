@model IEnumerable<TimeKeeper.Models.TKModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create", new { id = ViewBag.EmployeeId })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ClockedInOut)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ClockedInOut)
        </td>
        <td>
            @Html.ActionLink("Return to Employee", "../Employee/") |
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
